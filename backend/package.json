{
  "name": "feedeliza-plus",
  "version": "1.0.0",
  "type": "module",
  "description": "Clientes que voltam, marcas que crescem.",
  "main": "server.js",
  "imports": {
    "#~/*": "./src/*",
    "#middleware/*": "./src/middleware/*",
    "#config/*": "./src/config/*",
    "#domains/*": "./src/domains/*",
    "#services/*": "./src/services/*",
    "#utils/*": "./src/utils/*",
    "#models/*": "./models/*"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "setup-db": "node database/setup.js",
    "test:unit": "jest backend/tests/unit",
    "lint": "eslint \"**/*.js\" --ignore-pattern \"node_modules/\" --ignore-pattern \"dist/\" --fix"
  },
  "keywords": [
    "feedback",
    "restaurante",
    "nps",
    "qrcode",
    "whatsapp",
    "dashboard",
    "recompensas"
  ],
  "author": "Feedeliza+",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "googleapis": "^157.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.1.0",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "sequelize": "^6.32.1",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.12"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}

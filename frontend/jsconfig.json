{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "app/*": ["app/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "config/*": ["config/*"],
      "contexts/*": ["contexts/*"],
      "features/*": ["features/*"],
      "hooks/*": ["hooks/*"],
      "locales/*": ["locales/*"],
      "services/*": ["services/*"],
      "styles/*": ["styles/*"],
      "utils/*": ["utils/*"],
      "/dialogTheme": ["app/providers/contexts/theme/dialogTheme"],
      "/appBarDrawerTheme": ["app/providers/contexts/theme/appBarDrawerTheme"],
      "/cssBaselineTheme": ["app/providers/contexts/theme/cssBaselineTheme"],
      "/buttonTheme": ["app/providers/contexts/theme/buttonTheme"],
      "/cardTheme": ["app/providers/contexts/theme/cardTheme"],
      "/paperTheme": ["app/providers/contexts/theme/paperTheme"],
      "/formTheme": ["app/providers/contexts/theme/formTheme"],
      "/listTheme": ["app/providers/contexts/theme/listTheme"],
      "/dividerTheme": ["app/providers/contexts/theme/dividerTheme"],
      "/tableTheme": ["app/providers/contexts/theme/tableTheme"],
      "/tooltipTheme": ["app/providers/contexts/theme/tooltipTheme"]
    },
    "jsx": "react-jsx",
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}